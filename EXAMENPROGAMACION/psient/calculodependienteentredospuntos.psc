Proceso calculoDependienteEntreDosPuntos
	Definir x1,x2,y1,y2,m como reales;
	Escribir "Cuanto vale y2:";
	Leer y2;
	Escribir "Cuanto vale y1:";
	Leer y1;
	Escribir "Cuanto vale x2:";
	Leer x2;
	Escribir "Cuanto vale x1:";
	Leer x1;
	m<-(y2-y1)/(x2-x1);
	Escribir "Tu distancia es:";
	Escribir m;
FinProceso
